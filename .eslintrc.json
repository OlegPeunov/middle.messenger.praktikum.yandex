{
  "extends": "airbnb",
  "parser": "@typescript-eslint/parser",
  "plugins": ["@typescript-eslint"],
  "rules": {
    "no-param-reassign": 0,
    "max-len": [2, 100],
    "@typescript-eslint/no-unused-vars": 2,
    "indent": "off",
    "eol-last": 0,
    "no-multiple-empty-lines": ["error", { "max": 1, "maxEOF": 0 }]
   
  },
  "globals": {
    "window": true,
    "document": true,
    "HTMLElement": true
  }
}
